#!/bin/sh

. ${HEALTHCHECK_SCRIPTS:-/usr/share/openstack-tripleo-common/healthcheck}/common.sh

neutron_opflex_agent_status=$(supervisorctl -c /etc/neutron/neutron_opflex_supervisord.conf status neutron-opflex-agent | awk -F ' ' '{print $2}')


if [[ $neutron_opflex_agent_status != "RUNNING" ]]; then
   echo "fail"
   exit 1
fi
